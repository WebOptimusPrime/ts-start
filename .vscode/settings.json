{
  "jasmineExplorer.config": "jasmine.json",
  "jasmineExplorer.nodeArgv": ["-r", "ts-node/register/transpile-only"],
  "jasmineExplorer.env": {
    "TS_NODE_PROJECT": "./source/tsconfig.json"
  },
  "jasmineExplorer.logpanel": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "source/**/*.js": true,
    "**/.nyc*": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/package-lock.json": true
  }
}